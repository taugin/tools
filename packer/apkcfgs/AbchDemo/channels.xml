<?xml version="1.0" encoding="UTF-8"?>
<config>
    <global-plugins>
        <plugin name="pushsdk" desc="推送插件"/>
    </global-plugins>

    <srcapk>srcapks/AbchDemo.apk</srcapk>
    <finalname>AbchDemo</finalname>

    <channels>
        <channel>
            <!--基本参数-->
            <param name="suffix" value=".uc"/>
            <param name="splash" value="0"/>
            <param name="unity_splash" value="0"/>
            <param name="corner" value="rb"/>
            <!--渠道SDK参数-->
            <sdk>
                <param name="id" value="10"/>
                <param name="name" value="uc"/>
                <param name="sdk" value="ucsdk"/>
            </sdk>
            <!-- sdk-params 参数是写入到developer_config.properties中 -->
            <properties>
                <param name="UCGameId" value="544156" desc="GameId"/>
                <param name="UCCpId" value="22398" desc="CPId"/>
                <param name="UCDebugMode" value="false" desc="是否开启调试模式"/>
            </properties>
            <!--manifest参数会被写入到application下面的meta-data节点-->
            <manifest>
                <param name="APPID" value="1234567890"/>
                <param name="APPSECRIT" value="e569f0e23dfe6870853ddf7b53be39d6"/>
            </manifest>
            <!-- 渠道插件 -->
            <plugins>
                <plugin name="sharesdk" desc="分享插件"/>
            </plugins>
            <!--需要渠道单独处理的参数-->
            <spec-params>
                <param name="host" value="uc.com"/>
                <param name="schame" value="http"/>
            </spec-params>
            <!--中间件版本参数-->
            <sdk-version>
                <versionCode>1</versionCode>
                <versionName>1.0.0</versionName>
            </sdk-version>
        </channel>
        <channel>
            <!--基本参数-->
            <param name="suffix" value=""/>
            <param name="splash" value="0"/>
            <param name="unity_splash" value="0"/>
            <param name="corner" value="rb"/>
            <!--渠道SDK参数-->
            <sdk>
                <param name="id" value="10"/>
                <param name="name" value="ad"/>
                <param name="sdk" value="adsdk"/>
            </sdk>
            <!-- sdk-params 参数是写入到developer_config.properties中 -->
            <properties>
                <param name="countdown" value="5" desc="倒计时秒"/>
                <param name="splashimg" value="splash_img.png" desc="广告图片"/>
            </properties>
            <!--manifest参数会被写入到application下面的meta-data节点-->
            <manifest>
            </manifest>
            <!-- 渠道插件 -->
            <plugins>
            </plugins>
            <!--需要渠道单独处理的参数-->
            <spec-params>
            </spec-params>
            <!--中间件版本参数-->
            <sdk-version>
                <versionCode>1</versionCode>
                <versionName>1.0.0</versionName>
            </sdk-version>
        </channel>
    </channels>
</config>