<?xml version="1.0" encoding="UTF-8"?>
<config>
    <global-plugins>
        <plugin name="pushsdk" desc="推送插件"/>
    </global-plugins>

    <gameapk>games/AbchDemo.apk</gameapk>

    <channels>
        <channel>
            <param name="id" value="10"/>
            <param name="name" value="uc"/>
            <param name="sdk" value="ucsdk"/>
            <param name="desc" value="UC SDK"/>
            <param name="suffix" value=""/>
            <param name="splash" value="0"/>
            <param name="unity_splash" value="0"/>
            <param name="icon" value="rb"/>
            <param name="gamename" value="AbchDemo"/>
            <!-- sdk-params 参数是写入到developer_config.properties中 -->
            <sdk-params>
                <param name="UCGameId" value="544156" desc="GameId"/>
                <param name="UCCpId" value="22398" desc="CPId"/>
                <param name="UCDebugMode" value="false" desc="是否开启调试模式"/>
            </sdk-params>
            <!-- 渠道插件 -->
            <plugins>
                <plugin name="sharesdk" desc="分享插件"/>
            </plugins>
            <sdk-version>
                <versionCode>1</versionCode>
                <versionName>1.0.0</versionName>
            </sdk-version>
        </channel>
    </channels>
</config>